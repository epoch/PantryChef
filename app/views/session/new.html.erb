<div class="page-center">




	<h1>Login</h1>
	
	<%= form_tag session_index_path do  %>
	
		<p>
			<%= label_tag :username %>
			<%= text_field_tag :username %>
		</p>
	
		<p>
		<%= label_tag :password %>
		<%= password_field_tag :password %>
		</p>
	
		<button>Submit</button>
	
	<% end %>

		<p><%= flash[:notice]  %></p>


	</div>